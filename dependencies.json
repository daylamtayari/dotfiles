{
	"packageManager": {
		"debian": 	"apt",
		"fedora": 	"dnf",
		"arch": 	"pacman",
		"redHat": 	"yum",
		"suse":		"zypper",
		"installKey":	{
			"arch":		"-S",
			"default":	"install"
		}
	},
	"updates": {
		"updateCheck": {
			"fedora":	"check-update",
			"arch": 	"-Sy",
			"redHat":	"check-update",
			"suse":		"list-updates",
			"default": 	"update"
		},
		"upgrade": {
			"arch": 	"-Su",
			"redHat":	"update",
			"suse":		"update",
			"default": 	"upgrade"
		}
	},
	"packages": {
		"zsh": {
			"default":	"zsh"
		},
		"kitty": {
			"default": 	"kitty"
		},
		"imagemagick": {
			"fedora": 	"ImageMagick",
			"redHat":	"ImageMagick",
			"suse": 	"ImageMagick",
			"default": 	"imagemagick"
		},
		"neovim": {
			"default": 	"neovim"
		},
		"ranger": {
			"default": 	"ranger"
		},
		"firefox": {
			"debian": 	"firefox-esr",
			"suse": 	"MozillaFirefox",
			"default": 	"firefox"
		},
		"okular": {
			"default": 	"okular"
		},
		"jq": {
			"default": 	"jq"
		},
		"exa": {
			"default": "exa"
	}
}
