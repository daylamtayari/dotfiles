{
	"packageManager": {
		"apt": 		"apt",
		"dnf": 		"dnf",
		"pacman": 	"pacman",
		"yum": 		"yum",
		"zypper":	"zypper",
		"emerge":	"emerge",
		"installKey":	{
			"pacman":		"-S",
			"emerge":	"-u",
			"default":	"install"
		}
	},
	"updates": {
		"updateCheck": {
			"dnf":	"check-update",
			"pacman": 	"-Sy",
			"yum":	"check-update",
			"zyppper":		"list-updates",
			"emerge":	"--sync",
			"default": 	"update"
		},
		"upgrade": {
			"pacman": 	"-Su",
			"yum":	"update",
			"zyppper":		"update",
			"emerge":	"--update --deep --with-bdeps=y @world",
			"default": 	"upgrade"
		}
	},
	"packages": {
		"zsh": {
			"default":	"zsh"
		},
		"kitty": {
			"default": 	"kitty"
		},
		"imagemagick": {
			"dnf": 	"ImageMagick",
			"yum":	"ImageMagick",
			"zyppper": 	"ImageMagick",
			"default": 	"imagemagick"
		},
		"neovim": {
			"default": 	"neovim"
		},
		"ranger": {
			"default": 	"ranger"
		},
		"firefox": {
			"apt": 	"firefox-esr",
			"zyppper": 	"MozillaFirefox",
			"default": 	"firefox"
		},
		"okular": {
			"default": 	"okular"
		},
		"jq": {
			"default": 	"jq"
		},
		"exa": {
			"default": "exa"
	}
}
